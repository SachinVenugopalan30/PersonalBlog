--- 
import GitHub from './GitHub.astro';
import Instagram from './Instagram.astro';
import LinkedIn from './LinkedIn.astro';
import Steam from './Steam.astro';
import Mail from './Mail.astro';

interface Props {
  name: string;
}

const { name } = Astro.props;
---

{name === 'GitHub' && <GitHub />}
{name === 'Instagram' && <Instagram />}
{name === 'LinkedIn' && <LinkedIn />}
{name === 'Steam' && <Steam />}
{name === 'Mail' && <Mail />}
